{
  "target_urls": [{"https://books.toscrape.com/": false}],
  "elements": [
    {
      "name": "product_price",
      "tag": "p",
      "attributes": [
        {
          "name": "class",
          "value": "price_color"
        }
      ],
      "data_parsing": {
         "collect_text": true
      }
    },
    {
      "tag": "li",
      "attributes": [
        {
          "name": "class",
          "value": "active"
        }
      ],
      "data_parsing": {
         "collect_text": true
      }
    }
  ],
  "page_navigator":
    {
      "allowed_domains": ["books.toscrape.com"],
      "sleep_time": 0.5,
      "url_pattern": "catalogue\\/(?!.*\\bcategory\\b).*",
      "base_url": "https://books.toscrape.com/"
    }
}
