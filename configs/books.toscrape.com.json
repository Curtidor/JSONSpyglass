{
  "target_urls": ["https://books.toscrape.com/"],
  "elements": [
    {
      "name": "product_price",
      "tag": "p",
      "attributes": [
        {
          "name": "class",
          "value": "price_color"
        }
      ],
      "page_navigator": "global",
      "data_parsing": {
         "collect_text": true
      }
    }
  ],
  "page_navigator":
    {
      "allowed_domains": ["books.toscrape.com"],
      "sleep_time": 0.5,
      "url_pattern": "catalogue\\/(?!.*\\bcategory\\b).*",
      "target_elements": ["product_price"],
      "base_url": "https://books.toscrape.com/"
    }
}
