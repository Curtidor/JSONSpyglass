{
  "target_urls": [
    {
      "url": "https://books.toscrape.com/",
      "options": {
        "only_scrape_sub_pages": true,
        "render_pages": false
      }
    }
  ],
  "elements": [
    {
      "name": "Book Price",
      "css_selector": ".product_main p.price_color",
      "data_parsing": {
        "collect_text": true
      }
    },
    {
      "name": "Book Name",
      "css_selector": "h1",
      "data_parsing": {
        "collect_text": true
      }
    },
    {
      "name": "Stock",
      "search_hierarchy": [
        {
          "name": "class",
          "value": "col-sm-6 product_main"
        },
        {
          "name": "class",
          "value": "instock availability"
        }
      ],
      "data_parsing": {
        "collect_text": true
      }
    }
  ],
  "page_navigator": {
    "allowed_domains": ["books.toscrape.com"],
    "sleep_time": 0.5,
    "url_pattern": "catalogue\\/(?!.*\\bcategory\\b)[^\\/]+\\/index\\.html",
    "pagination_pattern": ["FEATURE NOT ADDED YET"],
    "base_url": "https://books.toscrape.com/"
  },
  "data_saving": {
    "csv": {
      "enabled": true,
      "file_path": "output.csv",
      "delimiter": ","
    }
  },
  "data_order": ["Book Name", "Book Price", "Stock"]
}
